{
  "name": "ショートカット検索!",
  "version": "1.0.0",
  "description":"shift+Ctrl+Fでショートカット検索！優秀なエンジニアはマウスを使わない！",
  "manifest_version": 3,
  "icons": {
    "16": "icon/icon16.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "action": {
    "default_icon": "icon/icon128.png"
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}

